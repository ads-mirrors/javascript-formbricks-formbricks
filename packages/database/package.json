{
  "name": "@formbricks/database",
  "private": true,
  "version": "0.1.0",
  "main": "./src/index.ts",
  "files": [
    "src",
    "zod",
    "zod-utils.ts"
  ],
  "scripts": {
    "clean": "rimraf .turbo node_modules",
    "db:migrate:deploy": "env DATABASE_URL=\"${MIGRATE_DATABASE_URL:-$DATABASE_URL}\" prisma generate && npm run apply-data-migrations",
    "db:migrate:dev": "dotenv -e ../../.env -- prisma migrate dev",
    "db:migrate:vercel": "if test \"$NEXT_PUBLIC_VERCEL_ENV\" = \"preview\" ; then env DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma db push --accept-data-loss ; else env DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate deploy ; fi",
    "db:push": "prisma db push --accept-data-loss",
    "db:up": "docker compose up -d",
    "db:setup": "pnpm db:up && pnpm prisma generate && pnpm apply-data-migrations",
    "db:start": "pnpm db:setup",
    "db:down": "docker compose down",
    "format": "prisma format",
    "generate": "prisma generate",
    "lint": "eslint ./src --fix",
    "build": "tsc && vite build",
    "generate-data-migration": "ts-node ./src/scripts/generate-data-migration.ts",
    "apply-data-migrations": "dotenv -e ../../.env -- ts-node ./src/scripts/apply-data-migrations.ts",
    "create-migration": "dotenv -e ../../.env -- ts-node ./src/scripts/create-migration.ts"
  },
  "dependencies": {
    "@prisma/client": "5.20.0",
    "@prisma/extension-accelerate": "1.2.1",
    "dotenv-cli": "7.4.2"
  },
  "devDependencies": {
    "@formbricks/config-typescript": "workspace:*",
    "@formbricks/eslint-config": "workspace:*",
    "@paralleldrive/cuid2": "2.2.2",
    "prisma": "5.20.0",
    "prisma-dbml-generator": "0.12.0",
    "prisma-json-types-generator": "3.1.1",
    "ts-node": "10.9.2",
    "vite": "6.0.3",
    "vite-plugin-dts": "4.3.0",
    "vite-plugin-node-polyfills": "0.22.0",
    "zod": "3.23.8",
    "zod-prisma": "0.5.4"
  }
}
